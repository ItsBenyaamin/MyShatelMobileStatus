<schemalist>
  <schema id="org.gnome.shell.extensions.MyShatelMobileStatus" path="/org/gnome/shell/extensions/MyShatelMobileStatus/">
    <key name="indicator-position" type="s">
      <default>"center"</default>
      <summary>Indicator position on the panel</summary>
    </key>

    <key name="indicator-index" type="i">
      <default>0</default>
      <summary>Indicator index in the selected position on panel</summary>
    </key>

    <key name="interval-duration" type="i">
      <default>30</default>
      <summary>Update interval in minutes</summary>
    </key>

    <key name="should-show-expire" type="b">
      <default>true</default>
      <summary>Should show expire date</summary>
    </key>

    <key name="curl-string" type="s">
      <default>''</default>
      <summary>Request that you copied</summary>
    </key>
  </schema>
</schemalist>
